{
  "manifest_version": 2,
  "name": "Hanzi Xiaobai",
  "version": "0.9.0",
  "description": "Learning Chinese one tab at a time",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
